\n    });\n  }\n\n  function handleLogout() {\n    logout();\n\n    setUserState({ user: null });\n\n    props.history.push('/');\n  }\n\n\n  return (\n    <StyledLayout>\n\n    <div className=\"App\">\n      <Header handleLogout={handleLogout} user={userState.user} />\n        <main>\n          <Switch>\n            <Route exact path=\"/\" render={props => \n              <HomePage {...props} />\n            } />\n            <Route exact path=\"/dashboard\" render={props => \n              userState.user ?\n              <DashboardPage />\n              :\n              <Redirect to=\"/login\" />\n            } />\n            <Route exact path=\"/signup\" render={props => \n              <SignupPage {...props} \n              handleSignUpOrLogin={handleSignUpOrLogin} />\n            } />\n            <Route exact path=\"/login\" render={props => \n              <LoginPage {...props} \n              handleSignUpOrLogin={handleSignUpOrLogin} />\n            } />\n            { animalState.animals.length > 0 && <Route exact path=\"/animals\" \n            render={props => \n            <IndexPage {...props} animals={animalState.animals}/>} \n            />}\n\n            <Route exact path=\"/animals/:id\" render={props => \n            <DetailPage {...props} animal={animalState.animals[props.match.params.id]}/>\n            }/>\n\n                {/* <Route exact path=\"/animals/:id\" \n              render={props => \n              <DetailPage {...props} />}\n            />}         */}\n\n          </Switch>\n        </main>\n      <Footer />\n    </div>\n\n    </StyledLayout>\n  );\n}\n\nexport default withRouter(App);\n","/Users/laurensuniga/GA-Class/Section 4/project-3-client/src/reportWebVitals.js",[],"/Users/laurensuniga/GA-Class/Section 4/project-3-client/src/components/Header/index.js",[],"/Users/laurensuniga/GA-Class/Section 4/project-3-client/src/components/Footer/index.js",[],"/Users/laurensuniga/GA-Class/Section 4/project-3-client/src/pages/HomePage/in