ss';\n\n\n\nfunction App(props) {\n\n  const [ userState, setUserState ] = useState({\n    user: getUser()\n  });\n\n\n async function getPetData(breed, limit) {\n  try {\n    const data = await getSearchData(breed, limit);\n    console.log(data)\n  } catch (error) {\n    \n  }\n}\n\n\n  function handleSignUpOrLogin() {\n    setUserState({\n      user: getUser()\n    });\n  }\n\n  function handleLogout() {\n    logout();\n\n    setUserState({ user: null });\n\n    props.history.push('/');\n  }\n\n  return (\n    <div className=\"App\">\n      <Header handleLogout={handleLogout} user={userState.user} />\n        <main>\n          <Switch>\n            <Route exact path=\"/\" render={props => \n              <HomePage />\n            } />\n            <Route exact path=\"/dashboard\" render={props => \n              userState.user ?\n              <DashboardPage />\n              :\n              <Redirect to=\"/login\" />\n            } />\n            <Route exact path=\"/signup\" render={props => \n              <SignupPage {...props} \n              handleSignUpOrLogin={handleSignUpOrLogin} />\n            } />\n            <Route exact path=\"/login\" render={props => \n              <LoginPage {...props} \n              handleSignUpOrLogin={handleSignUpOrLogin} />\n            } />\n          </Switch>\n        </main>\n      <Footer />\n    </div>\n  );\n}\n\nexport default withRouter(App);\n","/Users/laurensuniga/GA-Class/Section 4/project-3-client/src/reportWebVitals.js",[],"/Users/laurensuniga/GA-Class/Section 4/project-3-client/src/components/Header/index.js",[],"/Users/laurensuniga/GA-Class/Section 4/project-3-client/src/components/Footer/index.js",[],"/Users/laurensuniga/GA-Class/Section 4/project-3-client/src/pages/HomePage/index.js",[],"/Users/laurensuniga/GA-Class/Section 4/project-3-client/src/pages/LoginPage/index.js",[],"/Users/laurensuniga/GA-Class/Section 4/project-3-client/src/pages/SignupPage/index.js",[],"/Users/laurensuniga/GA-Class/Section 4/